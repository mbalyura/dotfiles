    "better-comments.tags": [
        {
            "tag": "!",
            "color": "#111111",
            "strikethrough": false,
            "backgroundColor": "#FF2D00"
        },
        {
            "tag": "?",
            "color": "#111111",
            "strikethrough": false,
            "backgroundColor": "#3498DB"
        },
        {
            "tag": "//",
            "color": "#98C379",
            // "color": "#474747",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "todo",
            "color": "#111111",
            "strikethrough": false,
            "backgroundColor": "#FF8C00"
        },
        {
            "tag": "*",
            "color": "#98C379",
            "strikethrough": false,
            "backgroundColor": "transparent"
        }
    ],

